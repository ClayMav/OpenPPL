{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./built",
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "target": "es5",
    "module": "commonjs",
    "checkJs": false,
    // "removeComments": true,
    "noEmit": true,
    // "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
  },
  "include": ["./App.tsx","./src/**/*","app.d.ts"],
}